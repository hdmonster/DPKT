<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="team.developer.official.dpkt">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
        <activity android:name=".RegisterActivity" />
        <activity android:name=".ListSolatWajib" />
        <activity android:name=".ListDetails" />
        <activity android:name=".AbsentSolat" />
        <activity android:name=".JadwalActivity" />
        <activity android:name=".KehadiranSolat" />
        <activity android:name=".AbsentTahajud" />
        <activity android:name=".KehadiranTahajud" />
        <activity android:name=".TahajudDetails"></activity>
    </application>

</manifest>